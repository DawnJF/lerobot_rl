        // export KMP_DUPLICATE_LIB_OK=TRUE
        {
            "name": "Python Debugger: learner_s",
            "type": "debugpy",
            "request": "launch",
            "module": "lerobot.scripts.rl.learner_s",
            "console": "integratedTerminal",
            "args": [
                "--config_path",
                "/liujinxin/mjf/lerobot/src/lerobot/scripts/rl/train_config_hilserl_s.json"
            ]
        },
        {
            "name": "actor",
            "type": "debugpy",
            "request": "launch",
            "module": "lerobot.scripts.rl.actor",
            "console": "integratedTerminal",
            "args": [
                "--config_path",
                "/home/robot/code/lerobot_rl/src/lerobot/config_files/train_ur_env_actor.json"
            ]
        },
        {
            "name": "gym_manipulator",
            "type": "python",
            "request": "launch",
